{
    "name": "phplrt/stream",
    "type": "library",
    "description": "The PHPLRT Stream package.",
    "license": "MIT",
    "keywords": [
        "streaming",
        "reader",
        "resource"
    ],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/phplrt/issues",
        "source": "https://github.com/phplrt/stream"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "phplrt/exception": "1.1.*|1.1.x-dev",
        "psr/http-message": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Stream\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "master": "1.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
